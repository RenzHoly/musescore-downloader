{
  "manifest_version": 2,
  "name": "MuseScore Downloader",
  "version": "0.0.2",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "homepage_url": "https://github.com/RenzHoly/musescore-downloader",
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "downloads",
    "webRequest",
    "declarativeContent",
    "*://musescore.com/user/*/scores/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "page_action": {
    "default_icon": "icons/icon32.png"
  }
}
